<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Copyright />
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Version>1.0.8.2</Version>
    <PackageTags>LDAP;Microsoft Active Directory;AD;.NET Standard;.NET Core;C#</PackageTags>
    <RepositoryUrl>https://github.com/comingnis/Coming.ActiveDirectoryHelper</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageProjectUrl>https://github.com/comingnis/Coming.ActiveDirectoryHelper</PackageProjectUrl>
    <Company>COMING COMPUTER ENGINEERING</Company>
    <Product>Coming.ActiveDirectoryHelper</Product>
    <Description>A helper library for accessing AD, which use Novell library in background.</Description>
    <PackageReleaseNotes>Some bugs fixed. IsAccountLockedOut() updated.</PackageReleaseNotes>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.0.8.2</AssemblyVersion>
    <FileVersion>1.0.8.2</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AntiXSS" Version="4.3.0" />
    <PackageReference Include="Novell.Directory.Ldap.NETStandard" Version="3.6.0" />
  </ItemGroup>

</Project>
